<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="js">
            <block class="Magento\Backend\Block\Template" template="BigBridge_ProductImport::js.phtml"
                   name="import_js_backend"/>
        </referenceContainer>
        <referenceContainer name="page.main.actions">
            <block class="Magento\Backend\Block\Template" name="start_import" template="BigBridge_ProductImport::actions/startimport.phtml"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block name="import.index" class="BigBridge\ProductImport\Block\Adminhtml\Import\Index" template="BigBridge_ProductImport::products/import.phtml"/>
        </referenceContainer>
    </body>
</page>
