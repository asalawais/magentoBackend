<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<body>
        <referenceContainer name="js">
            <block class="Magento\Backend\Block\Template" template="Maxsel_Emesa::js/js.phtml"
                   name="import_js_backend"/>
        </referenceContainer>
        <referenceContainer name="page.main.actions">
            <block class="Magento\Backend\Block\Template" name="action_buttons" template="Maxsel_Emesa::actions/buttons.phtml"/>
        </referenceContainer>
		<referenceContainer name="content">
			<block name="index.index" class="Maxsel\Emesa\Block\Adminhtml\Index\Index" template="Maxsel_Emesa::index/index.phtml"/>
		</referenceContainer>
	</body>
</page>
